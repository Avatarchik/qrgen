{
  "name": "qrgen",
  "version": "1.0.4",
  "description": "QrGen.cc is a free service that lets you create QR-Codes and shortened URLs from any link quickly and easily",
  "main": "index.js",
  "scripts": {
    "serve": "cd client && npx vue-cli-service serve",
    "build": "cd client && npx vue-cli-service build",
    "start": "node index.js",
    "server:watch": "tsc -w -p tsconfig.json",
    "server:start": "nodemon ./build/index.js",
    "pm2": "sudo pm2 start index.js --name qrgen.cc",
    "lint": "cd client && npx vue-cli-service lint"
  },
  "author": "Maximilian Schiller",
  "repository": {
    "type": "git",
    "url": "https://github.com/BetaHuhn/qrgen.git"
  },
  "homepage": "https://qrgen.cc",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.21.2",
    "mongoose": "^5.9.2",
    "nanoid": "^2.1.11",
    "request": "^2.88.2",
    "request-ip": "^2.1.3",
    "url-exists-deep": "^2.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-rate-limit": "^5.0.0",
    "@types/helmet": "0.0.47",
    "@types/mongoose": "^5.7.16",
    "@types/nanoid": "^2.1.0",
    "nodemon": "^2.0.3"
  }
}
